pipeline {
    agent any

    stages {
       
        stage('deploy') {
          steps{
           script {
             latestTag = sh(returnStdout:  true, script: "git tag --sort=-creatordate | head -n 1").trim()
             env.BUILD_VERSION = latestTag
             echo "env-BUILD_VERSION"
             echo "${env.BUILD_VERSION}"
            }
           }

    }
}
}
}
